{
	"luci-app-https-dns-proxy": {
		"description": "Grant UCI access for luci-app-https-dns-proxy",
		"read": {
			"cgi-io": [
				"exec"
			],
			"file": {
				"/usr/lib/lua/luci/https-dns-proxy/providers/*": [
					"read"
				],
				"/etc/init.d/dnsmasq *": [
					"exec"
				],
				"/etc/init.d/https-dns-proxy *": [
					"exec"
				]
			},
			"uci": [
				"https-dns-proxy"
			]
		},
		"write": {
			"uci": [
				"https-dns-proxy"
			]
		}
	}
}